{
  "name": "swapi-node",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start:dev": "npx nodemon",
    "start": "npm run build && node dist/index.js",
    "lint": "eslint . --ext .ts --fix",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "build:develop": "sls package --stage=develop",
    "deploy:develop": "sls deploy --stage=develop",
    "openapi": "sls openapi generate -o docs/openapi/openapi.json -f json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@types/mssql": "^8.1.2",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "adr": "^0.0.11",
    "babel-jest": "^29.6.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "husky": "^8.0.1",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-openapi-documentation-v2": "^0.4.6",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-s3-sync": "^1.17.1",
    "serverless-webpack": "^5.5.1",
    "swagger-ui-dist": "^5.3.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.51.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.385.0",
    "@aws-sdk/lib-dynamodb": "^3.385.0",
    "@middy/core": "^3.6.2",
    "@middy/http-error-handler": "^3.6.2",
    "@middy/http-json-body-parser": "^3.6.2",
    "@middy/validator": "^3.6.2",
    "@types/dotenv": "^8.2.0",
    "@types/uuid": "^8.3.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1229.0",
    "axios": "^1.4.0",
    "date-and-time": "^2.4.1",
    "dotenv": "^16.3.1",
    "middy": "^0.24.0",
    "swagger2openapi": "^7.0.8",
    "type-fest": "^3.13.1",
    "uuid": "^9.0.0"
  }
}
